<div class="text_font">
<h2>タイムスケジュール項目編集</h2>

    <%= form_with model:@schedule, url:plan_schedule_path(@schedule.plan_id,@schedule),local:true do |f| %>
        <div class="form-group">
            <label for="name">開始時刻</label>
            <%= f.time_field :start_time %>
        </div>
            <label for="name">終了時刻</label>
            <%= f.time_field :finish_time %>
        </div>
        <div class="form-group">
            <label for="name">項目</label>
            <%= f.text_field :plan_title%>
        </div>
        <div class="form-group">
            <label for="name">住所</label>
            <%= f.text_field :address%>
        </div>
        <div class="form-group">
            <label for="name">メモ</label>
            <%= f.text_area :memo%>
        </div>
        <div class="form-group">
            <%= f.submit '更新' %>
        </div>
        
       
    <% end %> 
    
    </div>